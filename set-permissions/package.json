{
  "name": "set-permissions",
  "version": "1.0.0",
  "description": "This Lambda function will set permissions to make transcoded videos publically available",
  "main": "index.js",
  "scripts": {
    "test": "run-local-lambda --file index.js --event tests/event.json",
    "deploy": "aws lambda update-function-code --function-name arn:aws:lambda:us-east-1:032665032862:function:set-permissions --zip-file fileb://Lambda-Deployment.zip",
    "predeploy": "zip -r Lambda-Deployment.zip * -x *.zip *.json *.log"
  },
  "dependencies": {
    "aws-sdk": "latest"
  },
  "devDependencies": {
    "run-local-lambda": "^1.1.1"
  },
  "author": "guitarbien",
  "license": "BSD-2-Clause"
}
